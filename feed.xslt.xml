<?xml version="1.0" encoding="utf-8"?><xsl:stylesheet version="3.0"
  xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
  xmlns:atom="http://www.w3.org/2005/Atom"
  xmlns:dc="http://purl.org/dc/elements/1.1/"><xsl:output method="html" version="1.0" encoding="UTF-8" indent="yes"/><xsl:template match="/"><html xmlns="http://www.w3.org/1999/xhtml"><head><meta charset="utf-8" /><meta name="viewport" content="width=device-width, initial-scale=1.0" /><meta http-equiv="X-UA-Compatible" content="IE=edge" /><title>Variant Minds - Site Feed</title><meta name="color-scheme" content="light dark" /><link rel="stylesheet" media="all" href="https://variantminds.au/assets/css/bootlace.min.css" integrity="" crossorigin="anonymous" /><style>a,a:hover{text-decoration:none;}.table{margin-top:0.5rem}.table thead td,.table thead th{border-top:none}</style></head><body><div class="container my-3"><header class="row"><h1 class="col h3 mt-5"><a href="https://variantminds.au/"><img style="width:auto;height:1.5rem;margin-bottom:0.25rem" src="https://variantminds.au/assets/favicon/favicon.svg" alt="Variant Minds" /><span> Variant Minds</span></a><small class="text-muted"> - Site Feed</small></h1></header><div class="row my-2 p-3"><div class="col rounded rounded-lg border shadow"><table class="table table-striped"><thead><tr><th scope="col">#</th><th scope="col">Title / Summary / Author</th><th scope="col">Published / / Categories</th><th scope="col">Last update</th></tr></thead><tbody><xsl:apply-templates select="/atom:feed/atom:entry"/></tbody></table></div></div><footer class="row"><small class="col text-muted text-center">&#169; <span class="copyright">2025</span> | <a href="https://variantminds.au/">Variant Minds</a></small></footer></div></body></html></xsl:template><xsl:template match="atom:entry"><tr><td scope="row" rowspan="3"><xsl:value-of select="position()" /></td><td><a href="{atom:link/@href}"><xsl:value-of select="atom:title"/></a></td><td><xsl:value-of select="atom:published"/></td><td><xsl:value-of select="atom:updated"/></td></tr><tr><td colspan="3"><xsl:value-of select="atom:summary"/></td></tr><tr><td><xsl:for-each select="atom:author"><span class="is-author"><xsl:value-of select="atom:name"/></span><xsl:if test="position() != last()"><br /></xsl:if></xsl:for-each></td><td colspan="2"><xsl:for-each select="atom:category"><span class="is-category"><xsl:value-of select="@term"/></span><xsl:if test="position() != last()">,</xsl:if><xsl:text></xsl:text></xsl:for-each></td></tr></xsl:template></xsl:stylesheet>
